{
    "manifestVersion": 1,
    "id": "build-release-task-hcl",
    "name": "HCL OneTest Azure Devops plugin",
    "version": "0.0.2",
    "publisher": "HCL",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Plugin for executing HCL OneTest test cases with Azure Devops.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/hcl.png"
    },
    "files": [
        {
            "path": "onetestui"
        },
        {
            "path": "onetestapi"
        },
        {
            "path": "onetestperformance"
        }
    ],
    "contributions": [
        {
            "id": "onetestui",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "onetestui"
            }
        },
        {
            "id": "onetestapi",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "onetestapi"
            }
        },
        {
            "id": "onetestperformance",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "onetestperformance"
            }
        }
    ]
}